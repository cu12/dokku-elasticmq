#!/usr/bin/env bash
export ELASTICMQ_IMAGE=${ELASTICMQ_IMAGE:="seayou/elasticmq"}
export ELASTICMQ_IMAGE_VERSION=${ELASTICMQ_IMAGE_VERSION:="latest"}
export ELASTICMQ_ROOT=${ELASTICMQ_ROOT:="/var/lib/dokku/services/elasticmq"}

export PLUGIN_COMMAND_PREFIX="sqs"
export PLUGIN_DATA_ROOT=$ELASTICMQ_ROOT
export PLUGIN_DATASTORE_PORTS=(9324)
export PLUGIN_DEFAULT_ALIAS="SQS"
export PLUGIN_ALT_ALIAS="DOKKU_ELASTICMQ"
export PLUGIN_IMAGE=$ELASTICMQ_IMAGE
export PLUGIN_IMAGE_VERSION=$ELASTICMQ_IMAGE_VERSION
export PLUGIN_SCHEME="http"
export PLUGIN_SERVICE="ElasticMQ"
